BEGIN {
	contacant = 0;
	i = 0
}

function norm(str) {
  return tolower(str);
}


match ($0 , /singer: (.*)/ , aux) {
	#print aux[1]
	if (aux[1] != "?")
			cant[aux[1]]
	}



END {
	# Não está a contar bem. Só conta um por música
	for (c in cant) {
				x = split(c,a,";",sep)
				for (i=1; i<=x; i++)
					aux[a[1]]
					print a[1]
	}

	for (x in aux) contacant ++

	printf ("No total são %s cantores.\n", contacant)
}
